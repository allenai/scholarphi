  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Run parser",
      "runtimeArgs": ["--nolazy", "-r", "ts-node/register"],
      "args": ["${workspaceFolder}/parse.ts", "equation", "x' + y^{\\prime}"],
      "cwd": "${workspaceRoot}",
      "sourceMaps": true,
      "outFiles": ["${workspaceFolder}/**/*.ts"]
    }
  ]
}
